    assert int("10", 36) == 36
    assert int("000Z", 36) == 35
    assert int("-000Z", 36) == -35
    assert int("+000Z", 36) == 35

    assert int("123", 0) == 123
    assert int("-123", 0) == -123
    assert int("+123", 0) == 123
    assert int("0b1101", 0) == 13
    assert int("-0b1101", 0) == -13
    assert int("+0b1101", 0) == 13
    assert int("0o123", 0) == 83
    assert int("-0o123", 0) == -83
    assert int("+0o123", 0) == 83
    assert int("0x1A", 0) == 26
    assert int("-0x1A", 0) == -26
    assert int(" +0x1a\n", 0) == 26

    try:
        int("1A", 10)
        assert False
    except ValueError:
        pass

    try:
        int("0b1101", 10)
        assert False
