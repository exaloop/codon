    assert sorted(d.items()) == [('blue', [2, 4]), ('red', [1]), ('yellow', [1, 3])]

    def constant_factory(value):
        return lambda: value

    d = defaultdict(constant_factory('<missing>'))
    assert d[10] == '<missing>'
