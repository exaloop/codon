@test
def test_complex_arrays():
    ncols = 2
    nrows = 2
    a = np.zeros((ncols, nrows), dtype=np.complex128)
    re = np.pi
    im = np.e
    a[:] = re + 1.0j * im

    f = test_dir + "/text_16.txt"
    np.savetxt(f, a)
    l = np.loadtxt(f, dtype=np.complex128)
    assert l.all() == a.all()

test_complex_arrays()

@test
def test_complex_negative_exponent():
    ncols = 2
