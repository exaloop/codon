    # Test matrices with reduced rank
    X = rnd.normal(size=(40, 10))
    for i in range(100):
        # Make a rank deficient matrix
        X = rnd.normal(size=(40, 10))
        X[:, 0] = X[:, 1] + X[:, 2]
        # Assert that matrix_rank detected deficiency
        assert alg.matrix_rank(X) == 9
        X[:, 3] = X[:, 4] + X[:, 5]
        assert alg.matrix_rank(X) == 8

