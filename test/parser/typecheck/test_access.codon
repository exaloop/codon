        if x > 5: print('f5', x, a)
        else: f5(x + x)

    def f6():
        nonlocal a
        print('f6', a)
        a += a
        print('f6', a, a.__class__.__name__)
        return a

    def f7():  # capture A
        a = 1

        def f7_1():  # capture A

            def f7_11():  # capture A
                global g
                nonlocal a
                a += a
                g += g
                print('f7_11', a, a.__class__.__name__)
            f7_11()
        f7_1()
        print('f7', a)

