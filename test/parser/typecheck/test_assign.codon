@dataclass
class Foo:
    a: int
    def __add__(self, i: int):
        print('add!')
        return Foo(self.a + i)
    def __iadd__(self, i: int):
        print('iadd!')
        self.a += i
        return self
    def __str__(self):
        return str(self.a)
f = Foo(3)
print(f + 2) #: add!
#: 5
f += 6 #: iadd!
print(f) #: 9

