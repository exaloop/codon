#%% walrus,barebones
def foo(x):
    return x * x
if x := foo(3):
    pass
if (x := foo(4)) and False:
    print('Nope')
if False and (x := foo(5)):
    print('Nope')
print(x) #: 16

a = [y := foo(1), y+1, y+2]
print(a) #: [1, 2, 3]

print({y: b for y in [1,2,3] if (b := (y - 1))}) #: {2: 1, 3: 2}
print(list(b for y in [1,2,3] if (b := (y // 3)))) #: [1]

