class F:
    @llvm
    def __int__(self: float) -> int:
        %0 = fptosi double %self to i64
        ret i64 %0
    def __float__(self: float):
        return self
    @llvm
    def __bool__(self: float) -> bool:
        %0 = fcmp one double %self, 0.000000e+00
        %1 = zext i1 %0 to i8
        ret i8 %1

@extend
